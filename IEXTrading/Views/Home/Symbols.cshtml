@model IEnumerable<IEXTrading.Models.Company>

<div class="headercenter">
    <br><br><h1><u>IEX Trading Application</u></h1>
            <h2><u>Symbols</u></h2></div>

<hr />

<table class="table">
    <thead>
        <tr>
            <th>
            <h2>
             Symbol
             </h2>
            </th>
            <th>
                <h2>
                    Name
                </h2>
                
            </th>
            <th>
                <h2>
                    Date
                </h2>
            </th>
            <th>
                <h2>
                    Type
                </h2>
            </th>

        </tr>
    </thead>

    <tbody>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.symbol)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.date)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.type)
                </td>
            </tr>
        }
        <tr>
            <td>
                <a id="dbopt" asp-controller="Home" asp-action="PopulateSymbols"><img src="~/images/button_save-data.png" /></a>
            </td>
        </tr>
    </tbody>
</table>
<script>
    alertDbSave(@ViewBag.dbSuccessComp);
</script>
